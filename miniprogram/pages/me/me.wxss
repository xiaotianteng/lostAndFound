.unlogin {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-top: 60%;
}

.unlogin .unlogin-image {
  width: 150rpx;
  height: 150rpx;
}
.unlogin .unlogin-button {
  margin-top: 50rpx;
  width: 80%;
  background-color: #00b618;
  color: #fff;
  height: 100rpx;
  line-height: 1.6;
}
.unlogin-body {
  background-color: #fff;
  height: 100vh;
  width: 100vw;
}

.info {
  background-color: #654644;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 100rpx 0;
}
.info .avatarUrl {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}

.info .nickName {
  color: #fff;
  margin-top: 30rpx;
}

.cell-list {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  width: 100%;
  top: -40rpx;
}
.cell {
  display: flex;
  justify-content: space-between;
  width: 80%;
  background-color: #fff;
  padding: 30rpx 20rpx;
  border-radius: 40rpx;
  align-items: center;
  margin-bottom: 20rpx;
}
.cell-left .cell-left-image {
  width: 50rpx;
  height: 50rpx;
}
.cell-left .cell-left-text {
  margin-left: 20rpx;
}

.cell-right .cell-right-image {
  width: 50rpx;
  height: 50rpx;
}

.cell-left {
  align-items: center;
}


.cell-body {
  position: relative;
}